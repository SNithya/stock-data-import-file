<form method="post">
  <div>
    <div>
      <span>ModelCode :</span>
      <input type="text" name="ModelCode" id="editPostModelCode" value="<%= post.ModelCode %>" />
    </div>
  <div>
<span>ItemCode :</span>
    <input type="text" name="ItemCode" id="editPostItemCode" value="<%= post.ItemCode %>" />
    </div>
<div>      
<span>ProductName :</span>
<input type="text" name="ProductName" id="editPostProductName" value="<%= post.ProductName %>" />
    </div>
<div>
<span>StockQty :</span>
      <input type="text" name="StockQty" id="editPostStockQty" value="<%= post.StockQty %>" />
    </div>
<div>
<span>Mrp :</span>
      <input type="text" name="Mrp" id="editPostMrp" value="<%= post.Mrp %>" />
    </div>
<div>
<span>CashandCarry :</span>
      <input type="text" name="CashandCarry" id="editPostCashandCarry" value="<%= post.CashandCarry %>" />
    </div>

    <div id='editPostSubmit'>
      <input type="submit" value="Send" />
    </div>
  </div>
</form>
